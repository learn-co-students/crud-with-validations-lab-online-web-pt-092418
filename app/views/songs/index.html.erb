<div id="all-songs"></div>
<div><%= link_to "Create Song", new_song_path %></div>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Genre</th>
      <th>Artist Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>

      <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.released %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td><%= link_to "Edit Song", edit_song_path(song) %></td>
        <td><%= link_to("Destroy", method: :delete)%></td>
      </tr>
        <% end %>

  </tbody>
</table>
